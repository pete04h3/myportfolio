erated":{"line":243,"column":26}}],"sources":{"partials/_var.scss":"$bg-color: magenta;\n\nbody {\n  background-color: var--$bg-color;\n  h1 {\n    color: black;\n  }\n}\n","partials/_header.scss":"header {\n  height: 0.3vh;\n  background: black;\n}\n","partials/_nav.scss":".menu__toggler {\n  position: absolute;\n  top: 20px;\n  left: 6.2px;\n  z-index: 999;\n  height: 28px;\n  width: 28px;\n  outline: none;\n  cursor: pointer;\n  display: flex;\n  align-items: center;\n}\n.menu__toggler span,\n.menu__toggler span::before,\n.menu__toggler span::after {\n  position: absolute;\n  content: \"\";\n  width: 28px;\n  height: 2.5px;\n  background: #fafafa;\n  border-radius: 20px;\n  transition: 500ms cubic-bezier(0.77, 0, 0.175, 1);\n}\n.menu__toggler span::before {\n  top: -8px;\n}\n.menu__toggler span::after {\n  top: 8px;\n}\n.menu__toggler.active > span {\n  background: transparent;\n}\n.menu__toggler.active > span::before,\n.menu__toggler.active > span::after {\n  background: #005c9c;\n  top: 0px;\n}\n.menu__toggler.active > span::before {\n  -webkit-transform: rotate(-225deg);\n  transform: rotate(-225deg);\n}\n.menu__toggler.active > span::after {\n  -webkit-transform: rotate(225deg);\n  transform: rotate(225deg);\n}\n\n.menu {\n  position: absolute;\n  left: -30%;\n  z-index: 998;\n  color: #005c9c;\n  background-color: black;\n  -webkit-clip-path: polygon(0 0, 100% 0, 85% 100%, 0% 100%);\n  clip-path: polygon(0 0, 100% 0, 85% 100%, 0% 100%);\n  width: 30%;\n  height: 100%;\n  padding: 100px;\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  transition: 300ms left cubic-bezier(0.77, 0, 0.175, 1);\n}\n@media only screen and (max-width: 600px) {\n  .menu {\n    width: 250px;\n    left: -250px;\n    padding: 50px;\n  }\n\n  #leftsidebar,\n  #rightsidebar {\n    display: none;\n  }\n}\n.menu.active {\n  left: 0;\n}\n.menu ul a li {\n  font-size: 1.4rem;\n  margin-bottom: 1rem;\n  text-decoration: none;\n  list-style: none;\n  color: white;\n}\n","sass/style.scss":"@use 'partials/var';\n@use 'partials/header';\n@use 'partials/mixins';\n@use 'partials/nav';\n\n/*\n   * BASIC STYLES\n   */\n*,\n*::before,\n*::after {\n  margin: 0;\n  padding: 0;\n  box-sizing: border-box;\n}\n\nbody {\n  min-height: 100vh;\n  background-image: linear-gradient(to top, black 0%, cyan 100%);\n  font-family: \"Open Sans\", sans-serif;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n}\n\nh1 {\n  color: #fafafa;\n  mix-blend-mode: overlay;\n}\n\n/* mobile-menu */\n\n#leftsidebar {\n  width: 3vw;\n  height: 100vh;\n  float: left;\n  position: fixed;\n  left: 0px;\n  background-color: black;\n}\n\n#rightsidebar {\n  width: 3vw;\n  height: 100vh;\n  float: right;\n  position: fixed;\n  right: 0px;\n  background-color: black;\n}\n\n/* footer */\n\nfooter {\n  background-color: black;\n  width: 100%;\n  height: 10vh;\n  position: fixed;\n  float: bottom;\n  bottom: 0px;\n}\n\n/* socials */\n\n.social {\n  display: block;\n  flex-direction: row;\n  clear: both;\n  color: white;\n  font-family: Roboto, Helvetica, sans-serif;\n  width: 100%;\n  height: 5vh;\n}\n\n.social-links {\n  padding: initial;\n  display: flex;\n  justify-content: center;\n  transform: scale(0.5);\n  margin: 0 auto;\n}\n\n.social-links li {\n  display: inline;\n  text-decoration: none;\n  font-size: 30px;\n  padding-left: 20px;\n  padding-right: 20px;\n}\n\n.social-links li a {\n  color: white;\n  -o-transition: 0.5s;\n  -ms-transition: 0.5s;\n  -moz-transition: 0.5s;\n  -webkit-transition: 0.5s;\n  transition: 0.5s;\n}\n\n.social-links li .facebook:hover {\n  color: #3b5998;\n}\n.social-links li .linkedin:hover {\n  color: #0077b5;\n}\n.social-links li .codepen:hover {\n  color: #fcd000;\n}\n.social-links li .github:hover {\n  color: lightgrey;\n}\n.social-links li .freecodecamp:hover {\n  color: darkgreen;\n}\n\n/*Main h1*/\n\nmain h1 {\n  color: antiquewhite;\n  margin: 0 auto;\n  position: fixed;\n  left: 0;\n  font-size: 320px;\n  font-family: monospace;\n  transform: skewX(10deg);\n  padding: 26px;\n  opacity: 0.6;\n}\n/* canvas AKA BIG SLIME BLACK DOT IN MIDDLE*/\ncanvas {\n  filter: drop-shadow(-2px 1px 60px black);\n}\n\n/*bubble1 header*/\n\n.bubble1 {\n  background-image: url(/static/bubble1.png);\n  background-size: 50% 10%;\n  background-repeat: repeat-x;\n  position: fixed;\n  transform: scale();\n  width: 100%;\n  filter: brightness(22.5);\n}\n"},"lineCount":null}},"error":null,"hash":"5c823691ecfc25ade5cb07afd68ace88","cacheData":{"env":{}}}